{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "checkout_completed",
  "type": "object",
  "required": ["user_id","currency"],
  "properties": {
    "user_id": { "type": "string" },
    "total_amount": { "type": "number", "minimum": 0 },
    "currency": { "type": "string", "enum": ["EUR", "SEK"] },
    "payment_method": { "type": "string", "enum": ["card", "invoice"] }
  }
}
